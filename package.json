{
  "name": "searxng-proxy",
  "version": "1.0.0",
  "description": "Proxy server for SearXNG meta-search engine with API key authentication",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsx src/index.ts",
    "lint": "eslint src/**/*.ts",
    "test": "npm run test:unit && npm run test:integration",
    "test:unit": "NODE_OPTIONS=--experimental-vm-modules jest tests/unit",
    "test:unit:watch": "NODE_OPTIONS=--experimental-vm-modules jest tests/unit --watch",
    "test:unit:coverage": "NODE_OPTIONS=--experimental-vm-modules jest tests/unit --coverage",
    "test:integration": "NODE_OPTIONS=--experimental-vm-modules jest tests/integration",
    "test:all": "NODE_OPTIONS=--experimental-vm-modules jest",
    "test:ci": "NODE_OPTIONS=--experimental-vm-modules jest --ci --coverage --runInBand",
    "start:searxng": "docker-compose up -d",
    "stop:searxng": "docker-compose down",
    "logs:searxng": "docker-compose logs -f",
    "restart:searxng": "docker-compose restart"
  },
  "keywords": ["searxng", "proxy", "api", "search", "meta-search", "docker"],
  "author": "loonylabs-dev",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/loonylabs-dev/searxng-proxy.git"
  },
  "bugs": {
    "url": "https://github.com/loonylabs-dev/searxng-proxy/issues"
  },
  "homepage": "https://github.com/loonylabs-dev/searxng-proxy#readme",
  "dependencies": {
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "node-fetch": "^3.3.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.5.0",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "@typescript-eslint/parser": "^6.4.0",
    "eslint": "^8.47.0",
    "jest": "^30.2.0",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.5",
    "ts-node": "^10.9.1",
    "tsx": "^4.7.0",
    "typescript": "^5.1.6"
  },
  "type": "module"
}
